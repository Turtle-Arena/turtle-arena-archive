// Turtle Arena team shaders. Based on MISSIONPACK demo shaders in team.shader

team_icon/Shell_red
{
	cull none
	surfaceparm nolightmap
	surfaceparm trans
	surfaceparm nomarks
	{
		map team_icon/Shell_red.png
		blendFunc Add
		rgbGen wave triangle 0.2 0.5 0 0.2
	}
}

